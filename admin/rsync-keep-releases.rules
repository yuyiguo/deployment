# Ignore current month release files
-rs /srv-dev/HG1408[a-z]
-rs /srv-*/[a-z][a-z]HG1408[a-z]

# Ignore previous month latest release files
-rs /srv-dev/HG1407[e-z]
-rs /srv-preprod/[a-z][a-z]HG1407[e-z]
-rs /srv-prod/[a-z][a-z]HG1407[e-z]

# Do not exclude the receiver's proxy files from current and previous releases
-r /srv-dev/HG140[1-8][a-z]/auth/proxy/proxy*.cert
-r /srv-*/[a-z][a-z]HG140[1-8][a-z]/auth/proxy/proxy*.cert

# Do not try to copy files from any other old releases
-s /srv-dev/HG131[0-2][a-z]
-s /srv-*/[a-z][a-z]HG131[0-2][a-z]
-s /srv-dev/HG140[1-7][a-z]
-s /srv-*/[a-z][a-z]HG140[1-7][a-z]
